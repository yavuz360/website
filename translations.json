  "element_mappings": {
    "index.html": {
      "title": "meta.site_description",
      "meta[name='description']": "meta.site_description",
      "meta[property='og:description']": "meta.og_description",
      ".brand__text": "hero.name",
      "nav a[href='#about']": "navigation.about_me",
      "nav a[href='#contact']": "navigation.contact",
      "nav a[href='/blogs']": "navigation.blogs",
      ".nav__toggle .visually-hidden": "navigation.toggle_menu",
      ".theme-toggle": "navigation.toggle_dark_mode",
      "h1": "hero.greeting + hero.name + hero.description",
      ".hero__content p": "hero.welcome",
      ".hero__cta a:first-child": "hero.cta_about",
      ".hero__cta a:last-child": "hero.cta_blogs",
      "#about h2": "about.title",
      "#about p:first-of-type": "about.intro",
      "#about p:nth-of-type(2)": "about.disclaimer",
      "#about p:nth-of-type(3)": "about.accounts + about.codeberg_account + about.github_account",
      "#about p:last-of-type": "about.hosting_thanks + about.github_pages + about.hosting_text",
      "#contact h2": "contact.title",
      "#contact p": "contact.message + contact.follow_up",
      ".copyright": "footer.made_with",
      ".to-top": "navigation.back_to_top"
    },
    "blogs.html": {
      "title": "blog.title",
      "meta[name='description']": "meta.blog_description",
      "meta[property='og:description']": "meta.blog_og_description",
      ".brand__text": "hero.name",
      "nav a[href='/#about']": "navigation.about_me",
      "nav a[href='/#contact']": "navigation.contact",
      "nav a[href='/blogs']": "navigation.blogs",
      ".nav__toggle .visually-hidden": "navigation.toggle_menu",
      ".theme-toggle": "navigation.toggle_dark_mode",
      "h1 .accent-gradient": "blog.title",
      "#category-latest": "blog.current",
      "#category-archived": "blog.archived",
      "#prev-page": "blog.previous",
      "#next-page": "blog.next",
      "#page-info": "blog.page",
      ".loading": "blog.loading",
      "#no-posts p": "blog.no_posts",
      ".disclaimer-footer p": "disclaimer.text",
      ".copyright": "footer.made_with",
      ".to-top": "navigation.back_to_top"
    },
    "404.html": {
      "title": "error_404.title",
      "meta[name='description']": "meta.error_description",
      "meta[property='og:description']": "meta.error_og_description",
      ".brand__text": "hero.name",
      "nav a[href='index.html#about']": "navigation.about_me",
      "nav a[href='index.html#contact']": "navigation.contact",
      "nav a[href='blogs.html']": "navigation.blogs",
      ".nav__toggle .visually-hidden": "navigation.toggle_menu",
      ".theme-toggle": "navigation.toggle_dark_mode",
      "h1 .accent-gradient": "error_404.title",
      "h1 span:last-child": "error_404.subtitle",
      "#cat-message": "error_404.cat_message",
      ".btn": "error_404.return_home",
      ".copyright": "footer.made_with",
      ".to-top": "navigation.back_to_top"
    },
    "script.js": {
      "setupControls currentBtn.textContent": "blog.current",
      "setupControls archivedBtn.textContent": "blog.archived",
      "renderBlogList .loading": "blog.loading",
      "showNoPosts": "blog.no_posts",
      "showError": "blog.loading",
      "generateLinkButtons h3": "blog.resources",
      "updatePaginationControls pageInfo": "blog.page"
    }
  }
